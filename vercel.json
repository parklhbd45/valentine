{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.JPG",
        "use": "@vercel/static"
      },
      {
        "src": "*.jpg",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }